{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
      "classes": true,
      "jsx": true
  },
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "react/jsx-indent": [2, 4],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "no-debugger": 0,
  },
  "plugins": [
    "react", "import"
  ],
  "parser": "babel-eslint"
}